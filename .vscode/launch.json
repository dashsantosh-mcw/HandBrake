  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "C:/Users/mcw/Desktop/hwaccel/HandBrakeCLI.exe",
      // "program": "C:/Users/mcw/Desktop/HandBrake/Build/vtune/HandBrakeCLI.exe",
      "args": [
        "--input",
        "C:/Users/mcw/Videos/bbb_sunflower_2160p_30fps_normal.mp4",
        "--output",
        "sample.mp4",
        "--preset",
        "Fast 720p30",
        "--encoder",
        "x265",
        // "--disable-hw-decoding",
        "--enable-hw-decoding",
        "mf",
        "--verbose",
        "--stop-at",
        "frame:1",
        "--no-comb-detect",
        "--no-decomb",
        "--rate",
        "30",
        "--vfr",
        "--verbose",
        "2",
        "--maxHeight",
        "2160",
        "--maxWidth",
        "3840",
        "--crop-mode",
        "none"
      ],
      "sourceFileMap": {
        "/home": "//wsl.localhost/Ubuntu/home"
      },
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "console": "integratedTerminal",
      "logging": { "exceptions": true, "processExit": true }
    }
  ]
}
